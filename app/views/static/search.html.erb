<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h1 class="text-center">Busca tu isapre</h1>
    <div class="well">
      <%= form_tag(plan_path, :method => 'get', id: "search-form") do %>
      <div class="form-group">
        <%= text_field_tag :search, params[:search], placeholder: "Busca planes" %>
        <%= submit_tag "Search" %>
        <% end %>
      </div>
    </div>
  </div>
</div>
